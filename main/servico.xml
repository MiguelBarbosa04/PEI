<?xml version="1.1" encoding="UTF-8"?>
<s:servicos xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="servico.xsd servico.xsd"
    xmlns:s="servico.xsd"
    xmlns:per="peritagem.xsd"
    xmlns:ent="entidade.xsd"
    xmlns:par="parametros.xsd">

    <s:servico>

        <s:peritagem id="123" id_parceiro="PTR2034" id_perito="PRT1028">
            <!-- Valor inteiro positivo -->
            <!-- O IDparceiro e IDperito tem que ser com dados alfanuméricos -->
            <!-- O IDparceiro e IDperito tem que ser com dados alfanuméricos -->
            <per:estado type="Não Realizado">
                <!-- Opção de "realizado" e "não realizado" -->
                <per:ocorrencia>Other</per:ocorrencia> <!-- Opcção inaccessible Location, missing vehicle, other  SE FOR other TEM DE TER EXPLICAÇÃO ATÉ 400 caracteres-->
                <per:explicacao>Teste</per:explicacao>
            </per:estado>
            <!--ADIICIONAR <per:dia>1/1/2020</per:dia> e por no peritagem.xsd-->
            <per:horaInicio>23:24</per:horaInicio>
            <per:horaFim>15:00</per:horaFim>
        </s:peritagem>

        <s:entidade tipo="Cliente">
            <!-- opção de "Cliente" ou "Oficina" -->
            <ent:Nome>Miguel Barbosa</ent:Nome>
        </s:entidade>

        <s:parametros>
            <!-- Nos parâmetros a opção é "Yes" ou "No" -->
            <par:parametro tipo="bateria">
                <par:conformidade>No</par:conformidade>
                <par:estado_bateria>Weak</par:estado_bateria>
            </par:parametro>

            <par:parametro tipo="nivelOleo">
                <par:conformidade>No</par:conformidade>
                <par:estado_oleo>Dirty</par:estado_oleo>
            </par:parametro>

            <par:parametro tipo="transmissao">
                <par:conformidade>No</par:conformidade>
                <par:estado_transmissao>Misfiring</par:estado_transmissao>
            </par:parametro>

            <par:parametro tipo="sistemaRefrigeracao">
                <par:conformidade>No</par:conformidade>
                <par:estado_sistemaRefrigeracao>Radiator Damaged</par:estado_sistemaRefrigeracao>
            </par:parametro>

            <par:parametro tipo="suporteMotor">
                <par:conformidade>No</par:conformidade>
                <par:estado_suporteMotor>Excess Vibration</par:estado_suporteMotor>
            </par:parametro>

            <par:parametro tipo="somMotor">
                <par:conformidade>No</par:conformidade>
                <par:estado_somMotor>Engine Auxiliary Noise</par:estado_somMotor>
            </par:parametro>

            <par:parametro tipo="gases">
                <par:conformidade>No</par:conformidade>
                <par:estado_gases>Black</par:estado_gases>
            </par:parametro>

            <par:parametro tipo="caixaVelocidades">
                <par:conformidade>No</par:conformidade>
                <par:estado_caixaVelocidades>Burning</par:estado_caixaVelocidades>
            </par:parametro>

            <par:parametro tipo="km">
                <par:num>100</par:num>
            </par:parametro>

        </s:parametros>

    </s:servico>
</s:servicos>